
@{
    Layout = "~/Views/Shared/_Formwizard_Layout.cshtml";
}

<!-- BEGIN CONTENT BODY -->
<div class="page-content">
<!-- BEGIN PAGE HEADER-->

<!-- BEGIN PAGE BAR -->
<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <a href="/Home/Index_Eproc">Home</a>
            <i class="fas fa-circle"></i>
        </li>
        <li>
            <span>Supplier Self Registration</span>
        </li>
    </ul>
</div>
<!-- END PAGE BAR -->

<!-- END PAGE HEADER-->
<div class="row">
<div class="col-md-12">
<div class="portlet light bordered" id="form_wizard_2">
<div class="portlet-title">
    <div class="caption">
        <i class="icon-layers font-red"></i>
        <span class="caption-subject font-red bold uppercase"> Supplier Self Registration Form -
            <span class="step-title"> Step 1 of 8 </span>
        </span>
    </div>
    <div class="actions">
        <a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">
            <i class="icon-cloud-upload"></i>
        </a>
        <a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">
            <i class="icon-wrench"></i>
        </a>
        <a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">
            <i class="icon-trash"></i>
        </a>
    </div>
</div>
<div class="portlet-body form">
    <form class="form-horizontal" action="#" id="submit_supplier_form" method="POST" enctype="multipart/form-data">
        <div class="form-wizard">
            <div class="form-body">
                <ul class="nav nav-pills nav-justified steps">
                    <li id="li1">
                        <a href="#tab1" data-toggle="tab" class="step">
                            <span class="number"> 1 </span><span class="desc"><i class="icon-check"></i> General Details </span>
                        </a>
                    </li>
                    <li id="li2">
                        <a href="#tab2" data-toggle="tab" class="step">
                            <span class="number"> 2 </span><span class="desc"><i class="icon-check"></i> Communication Details</span>
                        </a>
                    </li>
                    <li id="li3">
                        <a href="#tab3" data-toggle="tab" class="step active">
                            <span class="number"> 3 </span><span class="desc"><i class="icon-check"></i> Business Profile </span>
                        </a>
                    </li>
                    <li id="li4">
                        <a href="#tab4" data-toggle="tab" class="step active">
                            <span class="number"> 4 </span><span class="desc"><i class="icon-check"></i> Bank Details </span>
                        </a>
                    </li>
                    <li id="li5">
                        <a href="#tab5" data-toggle="tab" class="step">
                            <span class="number"> 5 </span><span class="desc"><i class="icon-check"></i> Director / Shareholders Details </span>
                        </a>
                    </li>
                    <li id="li6">
                        <a href="#tab6" data-toggle="tab" class="step">
                            <span class="number"> 6 </span><span class="desc"><i class="icon-check"></i> Prequalification Categories </span>
                        </a>
                    </li>
                    <li id="li7">
                        <a href="#tab7" data-toggle="tab" class="step">
                            <span class="number"> 7 </span><span class="desc"><i class="icon-check"></i> Litigation History </span>
                        </a>
                    </li>
                    <li id="li8">
                        <a href="#tab8" data-toggle="tab" class="step">
                            <span class="number"> 8 </span><span class="desc"><i class="icon-check"></i> Mandatory Documents </span>
                        </a>
                    </li>
                </ul>
                <div id="bar" class="progress progress-striped" role="progressbar">
                    <div class="progress-bar progress-bar-success"> </div>
                </div>
                <div class="tab-content">
                    <div class="alert alert-danger display-none">
                        <button class="close" data-dismiss="alert"></button> You have some form errors. Please check below.
                    </div>
                    <div class="alert alert-success display-none">
                        <button class="close" data-dismiss="alert"></button> Your form validation is successful!
                    </div>
                    <div id="regfeedback" style="display: none" data-dismiss="alert"></div>
                    <div class="tab-pane active" id="tab1">
                        <h3 class="block">Provide your general details</h3>
                        <div class="form-group">
                            <label class="control-label col-md-4">Business Type
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="form-control input-circle select2" id="ddlBusinesstype" name="ddlbzTypename">
                                    <option value="">--Choose Type--</option>
                                    <option value="1">Unit Trust</option>
                                    <option value="2">Broker</option>
                                    <option value="3">Agent</option>
                                </select>
                                <span class="help-block"> Provide your Business type </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">Vendor Type
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="form-control input-circle select2" id="ddlVendortype" name="ddlVendortype">
                                    <option value="">--Choose Type--</option>
                                    <option value="0">Local</option>
                                    <option value="1">Foreign</option>
                                </select>
                                <span class="help-block"> Provide your Vendor type </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Ownership Type
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="form-control input-circle input-circle select2" id="ddlOwnershiptype" name="ddlOwnershiptype">
                                    <option value="">--Choose Type--</option>
                                    <option value="1">Sole Ownership</option>
                                    <option value="2">Registered Company</option>
                                </select>
                                <span class="help-block"> Provide your Ownership type </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Certificate of Incorporation No
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle input-circle" name="txtCertofIncorp" id="txtCertofIncorp"/>
                                <span class="help-block"> Provide your Cert of Incorporation No </span>
                            </div>
                        </div>
                        

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Incorporation Date:
                                <span class="required"> * </span>
                            </label>
                            
                            <div class="col-md-6">
                                <div class="input-group input-medium date date-picker" data-date="12-02-2012" data-date-format="dd-mm-yyyy" data-date-viewmode="years">
                                    <input type="text" class="form-control input-circle-left" readonly  id="dtIncorp" name="dtIncorp" style="width: 550px">
                                    <span class="input-group-btn input-circle-right">
                                        <button class="btn default" type="button">
                                            <i class="fa fa-calendar"></i>
                                        </button>
                                    </span>
                                </div>
                                <span class="help-block"> Select Incorporation Date </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Operations Start Date:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <div class="input-group input-medium date date-picker" data-date="12-02-2012" data-date-format="dd-mm-yyyy" data-date-viewmode="years">
                                    <input type="text" class="form-control input-circle-left" readonly  id="dtOps" name="dtOps" style="width: 550px">
                                    <span class="input-group-btn input-circle-right">
                                        <button class="btn default" type="button">
                                            <i class="fa fa-calendar"></i>
                                        </button>
                                    </span>
                                </div>
                                <span class="help-block"> Select Operations Start Date </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">Language Code
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="input-circle form-control select2" id="ddlLanguageCode" name="ddlLanguageCode">
                                    <option value="">--Choose Language--</option>
                                    <option value="ENG">ENGLISH</option>
                                    <option value="ESP">ESPANOL</option>
                                    <option value="GER">GERMAN</option>
                                    <option value="FR">FRENCH</option>
                                </select>
                                <span class="help-block"> Provide your Language </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">Industry Group
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="input-circle form-control select2" id="ddlIndustrygroup" name="ddlIndustrygroup">
                                    <option value="">--Select Industry Group--</option>
                                    <option value="Software">ICT/ Software</option>
                                    <option value="Supplies">Supplies</option>
                                    <option value="Electronics">Electronics</option>
                                </select>
                                <span class="help-block"> Provide your Industry Group </span>
                            </div>
                        </div>
                      
                        <div class="form-group">
                            <label class="control-label col-md-4">Vision: &nbsp; <strong>(Max Characters: 250)</strong>
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <label id="lblCharleftTextarea1" style="color: red;"></label>
                                <textarea class="form-control input-circle" id="txtareaVision" name="txtareaVision" onkeyup="checkTextAreaVision(this, 250)" maxlength="250"></textarea>
                                <span class="help-block"> Your Vision </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">Mision: &nbsp; <strong>(Max Characters: 250)</strong>
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <label id="lblCharleftTextarea2" style="color: red;"></label>
                                <textarea class="form-control input-circle" id="txtareaMision" name="txtareaMision" onkeyup="checkTextAreaMision(this,250)" maxlength="250"></textarea>
                                <span class="help-block"> Your Mision </span>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" id="tab2">
                        <h3 class="block">Provide your Address details</h3>
                        <div class="form-group">
                            <label class="control-label col-md-4">P.O Box/Address
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtPoBox" id="txtPoBox" placeholder="P.O Box Office"/>
                                <span class="help-block"> Provide your PO Box address </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">Physical Location / Address 2
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtLocation" id="txtLocation"/>
                                <span class="help-block"> Provide your Physical Location Address </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Postal Code:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                @Html.Action("PostalCodeList", "Home")
                                <span class="help-block"> Provide choose your postal code </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                City:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtCity" id="txtCity" readonly="readonly" />
                                <span class="help-block"> Your Postal City </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Country:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                               @Html.Action("CountryList", "Home")
                                <span class="help-block"> Provide choose your Country </span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Website:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="url" class="form-control input-circle" name="txtwebsiteurl" id="txtwebsiteurl" placeholder="http://myexamplesite.com/" />
                                <span class="help-block"> Your Website Url </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Fixed Line Tel No:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtTelphNo" id="txtTelphNo" />
                                <span class="help-block"> Your Telephone Number</span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Building /House No:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="number" class="form-control input-circle" name="txtBuildNo" id="txtBuildNo" />
                                <span class="help-block"> Building / house Number </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Floor:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtFloor" id="txtFloor" />
                                <span class="help-block"> Building Floor </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Plot No:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtPlotNo" id="txtPlotNo" />
                                <span class="help-block"> Your Plot No </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Street / Road:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtStreetroad" id="txtStreetroad" />
                                <span class="help-block"> Location Street / Road </span>
                            </div>
                        </div>
                        

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Fax No:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtFaxNo" id="txtFaxNo" />
                                <span class="help-block"> Fax Number </span>
                            </div>
                        </div>

                    </div>
                    <div class="tab-pane" id="tab3">
                        <h3 class="block">Business profile details</h3>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Company Size:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="form-control input-circle select2" id="ddlCompanysize" name="ddlCompanysize">
                                    <option value="">--Select Company size--</option>
                                    <option value="10-15">10-15</option>
                                    <option value="15-30">15-30</option>
                                    <option value="30-above">30-above</option>
                                </select>
                                <span class="help-block"> Provide choose your company size </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Nominal Capital:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="form-control input-circle select2" id="ddlNominalCap" name="ddlNominalCap">
                                    <option value="">--Select Nominal Capital--</option>
                                    <option value="100000">100,000</option>
                                    <option value="200000">200,000</option>
                                    <option value="300000">300,000</option>
                                </select>
                                <span class="help-block"> Provide your Nominal capital </span>
                            </div>
                        </div>
                    

                        <div class="form-group">
                            <label class="control-label col-md-4">Dealership Type:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <select data-live-search="true" class="form-control input-circle select2" id="txtDealertype" name="txtDealertype">
                                    <option value="">--Select Dealer Type--</option>
                                    <option value="1">Manufacturer</option>
                                    <option value="2">Distributor</option>
                                    <option value="3">Partner</option>
                                    <option value="4">Reseller</option>
                                    <option value="5">Other</option>
                                </select>
                                <span class="help-block"> Provide Dealership type</span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Maximum Value of Business:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="number" class="form-control input-circle" name="txtMaxbzVal" id="txtMaxbzVal"/>
                                <span class="help-block"> Provide Maximum Business Value</span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Mobile No:
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-circle" name="txtCntPhoneno" id="txtCntPhoneno" />
                                <span class="help-block"> Provide contact mobile number</span>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="control-label col-md-4">Nature of Business: &nbsp; <strong>(Max Characters: 50)</strong>
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <label id="lblCharleftTextarea3" style="color: red;"></label>
                                <textarea class="form-control input-circle" id="txtareaNatureofBz" name="txtareaNatureofBz" onkeyup="checkTextAreaNtrBz(this,50)" maxlength="50"></textarea>
                                <span class="help-block"> Describe nature of your Business </span>
                            </div>
                        </div>
                        
                        <div class="form-group" style="display: none" id="divCollecteddataTest">
                            <label class="control-label col-md-4">Collected Data: &nbsp;
                                <span class="required"> * </span>
                            </label>
                            <div class="col-md-6">
                                <textarea class="form-control" id="txtCollectedData" name="txtCollectedData" readonly="readonly"></textarea>
                                <span class="help-block"> Collected Information so far </span>
                            </div>
                        </div>

                    </div>
                    <div class="tab-pane" id="tab4">
                        <h3 class="block">Provide details of your bank accounts below</h3>
                        <div class="row">
                            <div class="col-md-12">
                                @Html.Action("BankDetails", "Home")
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" id="tab5">
                        <h3 class="block">Provide complete list of directors/shareholders. Total ownership must be 100 % before submision</h3>
                        <div class="row">
                            <div class="col-md-12">
                                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                                <div class="portlet light portlet-fit bordered">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="icon-settings font-red"></i>
                                            <span class="caption-subject font-red sbold uppercase">Directors / Shareholders Details</span>
                                        </div>
                                    </div>
                                    <div class="portlet-body">
                                        <div class="table-toolbar">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="btn-group">
                                                        <button id="btn_add_director_new" class="btn green">
                                                            Add New
                                                            <i class="fa fa-plus"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="btn-group pull-right">
                                                        <button class="btn green btn-outline dropdown-toggle" data-toggle="dropdown">
                                                            Tools
                                                            <i class="fa fa-angle-down"></i>
                                                        </button>
                                                        <ul class="dropdown-menu pull-right">
                                                            <li>
                                                                <a href="javascript:;"> Print </a>
                                                            </li>
                                                            <li>
                                                                <a href="javascript:;"> Save as PDF </a>
                                                            </li>
                                                            <li>
                                                                <a href="javascript:;"> Export to Excel </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <table class="table table-striped table-hover table-bordered" id="tbl_directors">
                                            <thead>
                                            <tr>
                                                <th> Director Name </th>
                                                <th> Nationality</th>
                                                <th> ID Number</th>
                                                <th> Ownership Percentage (%) </th>
                                                <th> Phone Number </th>
                                                <th> Address </th>
                                                <th> Email </th>
                                                <th> Edit </th>
                                                <th> Delete </th>
                                            </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                                <!-- END EXAMPLE TABLE PORTLET-->
                            </div>
                        </div>
                    </div>
                <div class="tab-pane" id="tab6">
                    <h3 class="block">Please select atleast one product category. Your selection should be in line with an Open Invitation for Prequalification </h3>
                    <div class="row">
                        <div class="col-md-12">
                            <!-- BEGIN EXAMPLE TABLE PORTLET-->
                            <div class="portlet light portlet-fit bordered">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <i class="icon-settings font-red"></i>
                                        <span class="caption-subject font-red sbold uppercase">Prequalification Categories</span>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <div class="table-toolbar">
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">
                                                    Invitation for Prequalification (IFP) No:
                                                    <span class="required"> * </span>
                                                </label>
                                                <div class="col-md-6">
                                                    @Html.Action("IfpRequestsList", "Home")
                                                    <span class="help-block"> Choose IFP Number </span>
                                                </div>
                                            </div>
                                                
                                        </div>
                                    </div>
                                    <table class="table table-striped table-hover table-bordered" id="tbl_ifps">
                                        <thead>
                                        <tr>
                                            <th> Code</th>
                                            <th> Description</th>
                                            <th> Actions</th>
                                        </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                            <!-- END EXAMPLE TABLE PORTLET-->
                        </div>
                    </div>
                </div>
                <div class="tab-pane" id="tab7">
                    <h3 class="block">Provide a summary of any litigation and arbitration history for your organization</h3>
                    <div class="row">
                        <div class="col-md-12">
                            <!-- BEGIN EXAMPLE TABLE PORTLET-->
                            <div class="portlet light portlet-fit bordered">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <i class="icon-settings font-red"></i>
                                        <span class="caption-subject font-red sbold uppercase">Litigation History</span>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <div class="table-toolbar">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="btn-group">
                                                    <button id="btn_add_litigation_new" class="btn green">
                                                        Add New &nbsp;
                                                        <i class="fa fa-plus"></i>
