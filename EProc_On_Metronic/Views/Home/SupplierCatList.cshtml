@model IEnumerable<EProc_On_Metronic.Models.DropdownListsModel>

@{
    Layout = null;
}
@if (!Model.Any())
{
    <div class="alert alert-danger">no supplier categorization list yet, kindly check again later</div>
}
else
{
    <table class="table table-striped table-bordered table-hover dataTable xportabletable" id="supplierCatId">
        <thead>
        <tr>
            <th>Category ID</th>
            <th>Category Description</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Category)</td>
                <td>@Html.DisplayFor(modelItem => item.CategoryName)</td>
            </tr>
        }
        </tbody>
    </table>
}

